{
  "[blade]": {
    "editor.defaultFormatter": "shufo.vscode-blade-formatter",
    "editor.formatOnSave": true
  },
  "[css]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.formatOnSave": true,
    "editor.formatOnType": false,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "[elixir]": {
    "editor.defaultFormatter": "JakeBecker.elixir-ls",
    "editor.formatOnType": false,
    "editor.trimAutoWhitespace": false,
    "editor.wordBasedSuggestions": false,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true
  },
  "[json]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client",
    "editor.formatOnSave": false,
    "editor.tabSize": 4
  },
  "[powershell]": {
    "editor.defaultFormatter": "ms-vscode.powershell"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.tabSize": 4
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[vue]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "advancedNewFile.exclude": {
    "**/coverage": true,
    ".elixir_ls": true,
    ".mypy_cache": true,
    ".next": true,
    ".pytest_cache": true,
    "__pycache__": true,
    "_build": true,
    "bower_components": true,
    "build": true,
    "deps": true,
    "dist": true,
    "node_modules": true,
    "node_modules_electron": true,
    "rls": true,
    "target": true,
    "vendor": true
  },
  "cSpell.language": "en,de",
  "cSpell.userWords": [
    "Multithreading",
    "neovim",
    "nvim",
    "semibold"
  ],
  "dart.allowAnalytics": false,
  "dart.enableSnippets": true,
  "dart.flutterCreateOffline": true,
  "dart.flutterHotReloadOnSave": "always",
  "debug.javascript.terminalOptions": {
    "skipFiles": [
      "${workspaceFolder}/node_modules/**/*.js",
      "**/node_modules/**/*.js",
      "<node_internals>/**"
    ]
  },
  "debug.toolBarLocation": "docked",
  "editor.accessibilitySupport": "off",
  "editor.autoClosingQuotes": "always",
  "editor.codeActionsOnSave": {
    // "source.fixAll.stylelint": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "CaskaydiaCove NF",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.lineNumbers": "relative",
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {
    "comments": true,
    "other": true,
    "strings": true
  },
  "editor.rulers": [
    80
  ],
  "editor.scrollBeyondLastLine": false,
  "editor.suggestSelection": "first",
  //"editor.snippetSuggestions": "top",
  //"editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "elixirLS.enableTestLenses": true,
  "emmet.includeLanguages": {
    "blade": "html",
    "php": "html",
    "postcss": "css"
  },
  "eslint.packageManager": "yarn",
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.blade.php": "php",
    "*.html.eex": "html",
    ".env*": "shellscript",
    ".env*-cmdrc": "json",
    ".svg": "html",
    "Dockerfile*": "dockerfile"
  },
  "files.eol": "\n",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.elixir_ls": false,
    "**/.git": true,
    "**/.hg": true,
    "**/.mypy_cache": true,
    "**/.next": true,
    "**/.pyc": true,
    "**/.pytest_cache": true,
    "**/.svn": true,
    "**/.venv": true,
    "**/CVS": true,
    "**/__pycache__": true,
    "**/_build": true,
    "**/build": true,
    "**/coverage": true,
    "**/deps": true,
    "**/node_modules": true,
    "**/yarn*": true,
    "tags": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.enabled": false,
  "javascript.referencesCodeLens.enabled": false,
  "javascript.referencesCodeLens.showOnAllFunctions": false,
  "js/ts.implicitProjectConfig.strictNullChecks": true,
  "npm.packageManager": "yarn",
  "powershell.codeFormatting.useCorrectCasing": true,
  "powershell.integratedConsole.showOnStartup": false,
  "python.formatting.provider": "black",
  "search.showLineNumbers": true,
  "settingsSync.ignoredSettings": [
    "editor.fontSize",
    "workbench.activityBar.visible",
    "workbench.colorTheme",
    "window.titleBarStyle",
    "editor.fontFamily"
  ],
  "settingsSync.keybindingsPerPlatform": false,
  "stylelint.packageManager": "yarn",
  "tailwindCSS.colorDecorators": true,
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.experimental.classRegex": [
    [
      "(?:classnames|classNames|cn)\\(([^)]*)\\)",
      "(?:\"|'|`)([^(?:\"|'|`)]*)(?:\"|'|`)"
    ],
    [
      "\\[.+\\]:\\s*`([^)]*)\\`",
      "([^(?:\"|'|`)]+)\\s*"
    ]
  ],
  "tailwindCSS.includeLanguages": {
    "php": "html"
  },
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.tabs.enabled": true,
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.implementationsCodeLens.enabled": false,
  "typescript.referencesCodeLens.enabled": false,
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.surveys.enabled": false,
  "vetur.experimental.templateInterpolationService": true,
  "vetur.validation.templateProps": true,
  "vscode-neovim.highlightGroups.highlights": {
    "Conceal": "vim",
    "Directory": "vim",
    "IncSearch": {
      "backgroundColor": "theme.editor.findMatchBackground",
      "borderColor": "theme.editor.findMatchBorder"
    },
    "Search": {
      "backgroundColor": "theme.editor.findMatchHighlightBackground",
      "borderColor": "theme.editor.findMatchHighlightBorder"
    },
    "Substitute": "vim",
    "Visual": {
      "backgroundColor": "theme.editor.selectionBackground"
    }
  },
  "vscode-neovim.neovimExecutablePaths.win32": "C:\\users\\kanmii\\.bin\\neovim\\bin\\nvim.exe",
  "vscode-neovim.useWSL": false,
  "vsicons.dontShowNewVersionMessage": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.openFilesInNewWindow": "on",
  "window.openFoldersInNewWindow": "on",
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "Default Light+",
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "workbench.iconTheme": "vscode-icons",
  "workbench.settings.useSplitJSON": true,
  "workbench.startupEditor": "newUntitledFile",
  "zenMode.hideLineNumbers": false
}
